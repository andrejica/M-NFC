<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MnfcValues">

    <TextView
            android:text="Receiving value: "
            android:layout_width="81dp"
            android:layout_height="41dp"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="165dp" android:layout_marginStart="165dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="146dp"/>

    <Button
            android:text="Home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/back__home_mnfc"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp" android:layout_marginStart="160dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="340dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>